{
    "name": "kevupton/laravel-swagger",
    "description": "Laravel Swagger generator for the Model",
    "keywords": ["laravel", "swagger", "swagger-php", "model", "eloquent"],
    "type": "Extension",
    "license": "MIT",
    "authors": [
        {
            "name": "Kevin Upton",
            "email": "kevin@upton.tech"
        }
    ],
    "require": {
        "php": ">=5.3",
        "zircote/swagger-php": "^2.0",
        "doctrine/dbal": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "Kevupton\\LaravelSwagger\\": "src/Kevupton/LaravelSwagger/"
        },
        "files": [
            "src/Kevupton/LaravelSwagger/functions.php"
        ]
    },
    "minimum-stability": "stable",
    "require-dev": {
        "laravel/laravel": "^5.5",
        "laravel/lumen": "^5.5"
    }
}
